<!DOCTYPE html>

<html>


<head>

    <Title>Profile Page</Title>
    <meta charset="UTF-8"> 
    <link rel="stylesheet" type = "text/css" href = "/assets/css/Profile_Style.css">
    

</head>


<body>


  <div id = "Profile-Info">

    <img src = "/assets/images/Profile_Pic_Placeholder.png" alt = "Profile_Pic" height="256" width="256" >
  
    <h2><%= User.info.Nick_Name %></h2>
    <p><%= User.info.Desc %></p>
    <p><%= User.info.Province %>, <%= User.info.Country %></p>
    <p>Joined <%= User.info.Joined %></p>
    <button type = "button" id = "Message_Person">Message <%= User.info.Nick_Name %></button>

  </div>


  <div id = "Profile-Feed">

    <h2>Feed</h2>

   

      <ul>
      
      <%  User.Feed.forEach(function(elem){ %>


        <div name = "Tweets">

          
          <h4><%= elem.Content %></h4>
          <p><%= elem.Date %></p>


        </div>



      <% }) %>

       
      </ul> 
 

    

 
  </div>

</body>



</html>